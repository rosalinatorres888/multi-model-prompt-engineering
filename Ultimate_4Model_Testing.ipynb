    "    ultimate_keys = {\n",
    "        'OPENAI_API_KEY': 'OpenAI GPT',\n",
    "        'CLAUDE_API_KEY': 'Claude Sonnet',  \n",
    "        'GOOGLE_API_KEY': 'Google Gemini (Primary)',\n",
    "        'GOOGLE_API_KEY_BACKUP': 'Google Gemini (Backup)',\n",
    "        'LLAMA_API_KEY': 'Meta Llama'\n",
    "    }\n",
    "    \n",
    "    print(\"\\n🔐 ULTIMATE API Key Status:\")\n",
    "    print(\"=\" * 40)\n",
